{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Redis": "localhost:6379,defaultDatabase=0,ssl=false"
  },
  "Identity": {
    "ClientId": "mincloud.web.client",
    "ClientSecret": "mincloud.web.client.secret",
    "CallbackPath": "/signin-oidc",
    "Url": "https://localhost:5001",
    "Scopes": {
      "mincloud.readwrite": "MinCloud API read and write access"
    }
  },
  "DeepinApiUrl": "http://localhost:5000",
  "OpenApi": {
    "Endpoint": {
      "Name": "MinCloud Web Server V1"
    },
    "Document": {
      "Description": "The MinCloud Web Server HTTP API",
      "Title": "mincloud - Web Server HTTP API",
      "Version": "v1"
    },
    "Auth": {
      "ClientId": "mincloud.swagger.ui",
      "AppName": "MinCloud Swagger UI"
    }
  },
  "ReverseProxy": {
    "Routes": {
      "signalr": {
        "ClusterId": "signalrCluster",
        "Match": {
          "Path": "/hub/{**catch-all}"
        },
        "Transforms": [
          {
            "RequestHeaderRemove": "Cookie"
          }
        ]
      },
      "fileApi": {
        "ClusterId": "fileApiCluster",
        "Match": {
          "Path": "/api/files/{**catch-all}"
        },
        "Transforms": [
          {
            "RequestHeaderRemove": "Cookie"
          },
          {
            "PathPattern": "/api/v1/files/{**catch-all}"
          }
        ]
      }
    },
    "Clusters": {
      "signalrCluster": {
        "Destinations": {
          "signalrDestination": {
            "Address": "http://localhost:5000/"
          }
        }
      },
      "fileApiCluster": {
        "Destinations": {
          "fileApiDestination": {
            "Address": "http://localhost:5000/"
          }
        }
      }
    }
  }
}